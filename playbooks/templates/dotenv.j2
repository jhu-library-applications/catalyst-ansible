# borrow direct vars
BORROW_DIRECT_HOST={{ borrow_direct_host }}

# Flipper auth vars
FLIPPER_USERNAME={{ flipper_username }}
FLIPPER_PASSWORD={{ flipper_password }}

# Rollbar vars
ROLLBAR_ACCESS_TOKEN={{ rollbar_access_token }}
